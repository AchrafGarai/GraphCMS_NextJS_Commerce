{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "paths": {
      "@lib/*": ["lib/*"],
      "@hooks/*": ["hooks/*"],
      "@graphql/*": ["graphql/*"],
      
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@context/*": [
        "context/*"
      ],
      "@icons/*": [
        "components/icons/*"
      ], 
      "@svgs/*": [
        "components/svgs/*"
      ],
      "@ui/*": [
        "components/ui/*"
      ]
    }
  },
  "include": [
    "next-env.d.js",
    "**/*.ts",
    "**/*.tsx"
, "graphcms.config.js", "next.config.js", "postcss.config.js", "lib/get-page-data.js"  ],
  "exclude": [
    "node_modules"
  ],
}
